    "prettier": "^2.5.1",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.2.6",
    "typescript": "^5.1.6",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "worker-loader": "^3.0.8"
  },
