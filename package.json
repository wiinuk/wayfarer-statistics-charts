  "bugs": {
    "url": "https://github.com/wiinuk/wayfarer-statistics-charts/issues"
  },
  "homepage": "https://github.com/wiinuk/wayfarer-statistics-charts#readme",
  "devDependencies": {
    "@csstools/tokenizer": "^3.1.0",
    "@types/echarts": "^4.9.22",
    "@types/jest": "^27.5.2",
    "@types/jquery": "^2.0.60",
    "@types/jqueryui": "^1.11.32",
    "@types/leaflet": "^0.7.30",
    "@types/tampermonkey": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/experimental-utils": "^5.9.0",
